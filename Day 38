#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t); // Number of test cases
    while (t--) {
        int n;
        scanf("%d", &n); // Desired number of cuts
        if (n % 4 == 0) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }
    return 0;
}
