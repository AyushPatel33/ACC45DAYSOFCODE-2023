/*WAP for studen marks grading
        >91<100  A+
        >81<91  A
        >71<81  FAIL*/

#include<stdio.h>
int main()
{
    int x,y,z,sum,a;
    printf("Maths ");
    scanf("%d",&x);
    printf("Computer ");
    scanf("%d",&y);
    printf("Chemistry ");
    scanf("%d",&z);
    
    sum=x+y+z;
    
//a = percentage obtained
    a = sum * 100/300;
    {printf("Student's percentage = %d \n", a);}
    
    if(a>91 && a<100)
    {printf("YOU GOT A+ GRADE");}
    
    else if(a>81 && a <91)
    {printf("YOU GOT A GRADE");}

    else if(a>71 && a <81)
    {printf("YOU ARE FAIL");}
    
    return 0;
    
}
