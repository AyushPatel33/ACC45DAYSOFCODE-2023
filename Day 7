#include <stdio.h>

int main() {
    int T; // Number of test cases
    scanf("%d", &T);

    while (T--) {
        int X, Y; // Following and follower count
        scanf("%d %d", &X, &Y);

        if (X > 10 * Y) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
