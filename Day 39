#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int a1, a2, b1, b2;
        scanf("%d %d %d %d", &a1, &a2, &b1, &b2);

        if (a1 > b2 && a2 > b1) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
